{
    "queue_system": {
        "type": "M/M/c",
        "parameters": {
            "arrival_rate": {
                "normal": {
                    "lambda": 8,  // Средняя скорость поступления (вызовов в единицу времени)
                    "distribution": "Poisson" // Распределение вероятностей для прибытия
                }
            },
            "service_rate": {
                "normal": {
                    "mu": 12, // Средняя скорость обслуживания (вызовов в единицу времени)
                    "distribution": "Exponential" // Распределение вероятностей для обслуживания
                }
            },
            "num_servers": 3,  // Количество обслуживающих каналов
            "buffer_size": 20  // Размер буфера (если есть)
        },
        "customer_classes": [
            {
                "class_id": 1,
                "priority": 1,
                "arrival_rate": 5, // Скорость поступления для этого класса
                "service_time": 0.6 // Среднее время обслуживания
            },
            {
                "class_id": 2,
                "priority": 2,
                "arrival_rate": 3, // Скорость поступления для этого класса
                "service_time": 1.0 // Среднее время обслуживания
            }
        ],
        "state": {
            "current_customers": 10, // Текущее количество клиентов в системе
            "waiting_customers": 4,   // Количество клиентов в очереди
            "busy_servers": 3          // Количество занятых серверов
        },
        "performance_metrics": {
            "average_waiting_time": {
                "class_1": 1.2, // Среднее время ожидания для класса 1 (в минутах)
                "class_2": 0.8  // Среднее время ожидания для класса 2 (в минутах)
            },
            "average_queue_length": {
                "class_1": 3, // Средняя длина очереди для класса 1
                "class_2": 1   // Средняя длина очереди для класса 2
            },
            "system_utilization": 0.85, // Утилизация системы (от 0 до 1)
            "blocking_probability": 0.1   // Вероятность блокирования для клиентов
        }
    }
}